@model IEnumerable<EugenieWeb.Web.Areas.Management.Helpers.WebApiModels.Deal>

<div class="table-responsive">
    <div id="condensedTable_wrapper" class="dataTables_wrapper form-inline no-footer">
        <table class="table table-hover table-condensed dataTable no-footer" id="condensedTable" role="grid">
            <thead>
                <tr role="row">
                    <th class="sorting_asc" tabindex="0" rowspan="1" colspan="1">Дата</th>
                    <th class="sorting" tabindex="0" rowspan="1" colspan="1">Стойност</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var deal in Model)
                {
                    @Html.DisplayFor(m => deal)
                }
            </tbody>
        </table>
    </div>
</div>